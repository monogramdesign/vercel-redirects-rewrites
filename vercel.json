{
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard",
      "permanent": false,
      "has": [
        {
          "type": "cookie",
          "key": "my-router-cookie",
          "value": "redirect"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sample-rewrite",
      "destination": "/home"
    },
    {
      "source": "/",
      "destination": "https://redirect-rewrite-demo.monogram.dev/dashboard",
      "has": [
        {
          "type": "cookie",
          "key": "my-router-cookie",
          "value": "rewrite"
        }
      ]
    }
  ]
}
