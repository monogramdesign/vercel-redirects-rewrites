{
	"redirects": [
		{
			"source": "/",
			"destination": "/dashboard",
			"permanent": false,
			"has": [
				{
					"type": "cookie",
					"key": "my-http-only-cookie",
					"value": "redirect"
				}
			]
		}
	],
	"rewrites": [
		{
			"source": "/sample-rewrite",
			"destination": "/"
		},
		{
			"source": "/",
			"destination": "https://redirect-rewrite-demo.monogram.dev/dashboard",
			"has": [
				{
					"type": "cookie",
					"key": "my-http-only-cookie",
					"value": "rewrite"
				}
			]
		}
	]
}
